{
  "name": "@windingtree/videre-sdk",
  "version": "0.3.0",
  "description": "Videre Protocol SDK",
  "author": "mfw78 <mfw78@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "videre",
    "p2p",
    "waku",
    "windingtree",
    "markets"
  ],
  "scripts": {
    "protoc": "protoc --ts_out src/proto --proto_path ./src/proto ./src/proto/*.proto",
    "build:protoc": "mkdir proto && protoc --ts_out proto --proto_path ./src/proto ./src/proto/*.proto",
    "build:typechain": "typechain --target ethers-v5 --glob './node_modules/@windingtree/videre-contracts/artifacts/**/*.json' --out-dir typechain",
    "build": "yarn build:typechain && yarn build:protoc && cp -pR ./src/utils ./"
  },
  "files": [
    "proto",
    "typechain",
    "utils"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@windingtree/videre-contracts": "^0.1.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@protobuf-ts/plugin": "^2.5.0",
    "ethers": "^5.6.5"
  }
}
